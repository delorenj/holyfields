{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://33god.dev/schemas/agent/session.started.v1.json",
  "title": "Agent Session Started Event",
  "description": "New agent session began",
  "type": "object",
  "allOf": [
    { "$ref": "../_common/base_event.v1.json" }
  ],
  "properties": {
    "event_type": {
      "type": "string",
      "const": "agent.session.started",
      "description": "Event type discriminator"
    },
    "payload": {
      "type": "object",
      "properties": {
        "agent_name": {
          "$ref": "../_common/types.v1.json#/$defs/agent_name",
          "description": "Name of the agent"
        },
        "session_key": {
          "$ref": "../_common/types.v1.json#/$defs/session_key",
          "description": "Session identifier"
        },
        "channel": {
          "$ref": "../_common/types.v1.json#/$defs/channel_type",
          "description": "Channel the session is on"
        },
        "model": {
          "$ref": "../_common/types.v1.json#/$defs/model_name",
          "description": "AI model used in this session"
        }
      },
      "required": ["agent_name", "session_key"]
    }
  },
  "required": ["event_type", "payload"]
}
